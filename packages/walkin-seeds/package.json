{
  "name": "@walkinserver/walkin-seeds",
  "version": "0.1.1",
  "description": "Walkin Platform GraphQL/REST Server",
  "license": "MIT",
  "scripts": {
    "migration:run": "ts-node ../../node_modules/typeorm/cli.js migration:run -f ormconfig.migrations.ts",
    "migration:generate": "ts-node ../../node_modules/typeorm/cli.js migration:generate -f ormconfig.migrations.ts -n",
    "migration-selective:run": "ts-node ../../node_modules/typeorm/cli.js migration:run -f ormconfig.selective.migrations.ts",
    "migration:show": "ts-node ../../node_modules/typeorm/cli.js migration:show -f ormconfig.migrations.ts",
    "migration:create": "ts-node ../../node_modules/typeorm/cli migration:create -f ormconfig.migrations.ts -n",
    "migration:revert": "ts-node ../../node_modules/typeorm/cli migration:revert -f ormconfig.migrations.ts",
    "migration-selective:revert": "ts-node ../../node_modules/typeorm/cli migration:revert -f ormconfig.selective.migrations.ts",
    "seeds:run": "ts-node ../../node_modules/typeorm/cli.js migration:run -f ormconfig.seeds.ts ",
    "seeds:create": "ts-node ../../node_modules/typeorm/cli migration:create -f ormconfig.seeds.ts -n",
    "seeds:revert": "ts-node ../../node_modules/typeorm/cli migration:revert -f ormconfig.seeds.ts",
    "fixes:run": "ts-node ../../node_modules/typeorm/cli migration:run -f ormconfig.fixes.ts",
    "fixes:show": "ts-node ../../node_modules/typeorm/cli migration:show -f ormconfig.fixes.ts",
    "fixes:create": "ts-node ../../node_modules/typeorm/cli migration:create -f ormconfig.fixes.ts -n",
    "fixes:revert": "ts-node ../../node_modules/typeorm/cli migration:revert -f ormconfig.fixes.ts"
  },
  "dependencies": {
    "@graphql-codegen/cli": "1.9.1",
    "@graphql-codegen/schema-ast": "1.9.1",
    "@graphql-codegen/typescript": "1.9.1",
    "@graphql-codegen/typescript-resolvers": "1.9.1",
    "@types/chance": "^1.0.7",
    "@walkinserver/walkin-core": "*",
    "@walkinserver/walkin-platform-server": "*",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "chance": "^1.1.3",
    "dotenv": "^8.1.0",
    "graphql-tag-pluck": "^0.8.4",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^8.3.0",
    "typeorm": "0.2.24",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.3.0"
  }
}
